watr:Constituent-Organics
    a watr:Class ; 
    a watr:Constituent-Organics ;
    a sh:NodeShape ;
    rdfs:comment "Constituent-Organics" ;
    rdfs:label "Constituent-Organics" ;
    rdfs:subClassOf s223:Medium-Constituent ;
.
watr:Constituent-Solids
    a watr:Class ;
    a watr:Constituent-Solids ;
    a sh:NodeShape ;
    rdfs:comment "Constituent-Solids" ;
    rdfs:label "Constituent-Solids" ;
    rdfs:subClassOf s223:Medium-Constituent ;
.
watr:Constituent-SuspendedSolids
    a watr:Class ;
    a watr:Constituent-SuspendedSolids ;
    a sh:NodeShape ;
    rdfs:comment "Suspended solids are particulate matter that is not dissolved in water and can be removed by filtration including organic and inorganic particles." ;
    rdfs:label "Constituent-SuspendedSolids" ;
    rdfs:subClassOf watr:Constituent-Solids ;
.
watr:Sludge-MixedLiquor
    a watr:Class ;
    a watr:Sludge-MixedLiquor ;
    a sh:NodeShape ;
    rdfs:comment "Mixed liquor is the mixture of wastewater and activated sludge in the aeration basin of a wastewater treatment plant. It contains suspended solids and water." ;
    rdfs:label "Sludge-MixedLiquor" ;
    rdfs:subClassOf s223:Fluid-Sludge ;
    s223:composedOf [
        a s223:QuantifiableProperty ;
        s223:ofConstituent watr:Constituent-SuspendedSolids ;
        qudt:hasQuantityKind quantitykind:MassFraction ;
        qudt:hasUnit unit:PERCENT ;
        rdfs:label "Suspended Solids Fraction"
    ] ;
    s223:composedOf [
        a s223:QuantifiableProperty ;
        s223:ofConstituent s223:Constituent-H2O ;
        qudt:hasQuantityKind quantitykind:MassFraction ;
        qudt:hasUnit unit:PERCENT ;
        rdfs:label "Water Fraction"
    ] ;
.