    a s223:EnumerationKind-Role ;
    a watr:Class ;
    a s223:Role-Feed ;
    a sh:NodeShape ;
    rdfs:label "Feed" ;
    rdfs:subClassOf s223:EnumerationKind-Role ;
.
watr:Role-Permeate 
    a s223:EnumerationKind-Role ;
    a watr:Class ;
    a watr:Role-Permeate ;
    a sh:NodeShape ;
    rdfs:label "Permeate" ;
    rdfs:subClassOf s223:EnumerationKind-Role ;
. 
