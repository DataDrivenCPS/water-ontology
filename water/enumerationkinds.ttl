@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix s223: <http://data.ashrae.org/standard223#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix equip: <urn:nawi-water-ontology/equip#> .
@prefix quantitykind: <http://qudt.org/vocab/quantitykind/> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix unit: <http://qudt.org/vocab/unit/> .
@prefix watr: <urn:nawi-water-ontology#> .

<urn:nawi-water-ontology/enumerationkinds>
    a owl:Ontology ;
    rdfs:label "NAWI Water EnumerationKinds Ontology" ;
    rdfs:comment "An ontology for enumerationkinds used in the NAWI project" ;
    owl:imports <http://data.ashrae.org/standard223/1.0/model/all> ;
.

watr:Role-Backwash 
    a s223:EnumerationKind ;
    a watr:Role-Backwash ; 
    rdfs:subClassOf s223:EnumerationKind-Role ;
    rdfs:label "Role-Backwash" ;
.
watr:Role-MakeUp 
    a s223:EnumerationKind-Role ;
    a watr:Class ;
    a watr:Role-Makeup ;
    a sh:NodeShape ;
    rdfs:label "MakeUp" ;
    rdfs:subClassOf s223:EnumerationKind-Role ;
.

watr:Role-Feed 
    a s223:EnumerationKind-Role ;
    a watr:Class ;
    a s223:Role-Feed ;
    a sh:NodeShape ;
    rdfs:label "Feed" ;
    rdfs:subClassOf s223:EnumerationKind-Role ;
.
watr:Role-Permeate 
    a s223:EnumerationKind-Role ;
    a watr:Class ;
    a watr:Role-Permeate ;
    a sh:NodeShape ;
    rdfs:label "Permeate" ;
    rdfs:subClassOf s223:EnumerationKind-Role ;
. 
# TODO: Move to process type
watr:Role-Aeration 
    a s223:EnumerationKind-Role ;
    a watr:Class ;
    a watr:Role-Aeration ;
    a sh:NodeShape ;
    rdfs:label "Aeration" ;
    rdfs:subClassOf s223:EnumerationKind-Role ;
.
watr:EnumerationKind-Runtime
    a s223:EnumerationKind ;
    a watr:Class ;
    a s223:EnumerationKind-Runtime ;
    a sh:NodeShape ;
    rdfs:label "Runtime" ;
    rdfs:subClassOf s223:EnumerationKind ;
.
